{
  "name": "@helsenorge/autosuggest",
  "version": "31.7.0",
  "main": "index.js",
  "publishConfig": {
    "directory": "lib",
    "access": "public"
  },
  "author": "Norsk helsenett SF",
  "maintainers": [
    "ROX"
  ],
  "description": "Autosuggest-komponent for Ã¥ vise forslag i forbindelse med inputfelt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/helsenorge/core-frontend-packages"
  },
  "type": "module",
  "scripts": {
    "prestart": "rimraf dist",
    "start": "node node_modules/@helsenorge/library-build/library-scripts.js start",
    "clean": "rimraf lib",
    "typed-scss-modules": "typed-scss-modules \"src/**/*.module.scss\" --nameFormat none --exportType default --includePaths node_modules ../../../node_modules",
    "prebuild": "npm-run-all clean typed-scss-modules",
    "build": "tsc",
    "postbuild": "node node_modules/@helsenorge/library-build/library-scripts.js copy --root=README.md --include=src/**/*.{scss,scss.d.ts}",
    "watch": "tsc --watch",
    "eslint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint src/**/*.{css,scss}",
    "stylelint:fix": "npm run stylelint -- --fix",
    "prettier": "prettier --check src/**/*.{js,jsx,ts,tsx,css,scss,md,json}",
    "prettier:fix": "npm run prettier -- --write",
    "pretypecheck": "tsc -v",
    "typecheck": "tsc --noEmit",
    "test": "node node_modules/@helsenorge/library-build/library-scripts.js test",
    "test:update": "node node_modules/@helsenorge/library-build/library-scripts.js test --u",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@helsenorge/core-utils": "^32.0.0-beta",
    "@helsenorge/designsystem-react": "^8.0.0-beta",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "optionalDependencies": {
    "react-hook-form": "^7.52.0"
  },
  "devDependencies": {
    "@helsenorge/core-utils": "^31.7.0",
    "@helsenorge/designsystem-react": "8.0.0-beta.2",
    "@helsenorge/library-build": "^31.3.0",
    "react-hook-form": "^7.52.0"
  },
  "dependencies": {
    "@types/react-autosuggest": "^10.1.11",
    "react-autosuggest": "^10.1.0"
  }
}
