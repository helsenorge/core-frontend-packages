{
  "name": "@helsenorge/core-utils",
  "version": "36.7.1",
  "description": "Samling av ulike verktøy og hjelpefunksjoner for bruk på Helsenorge",
  "repository": {
    "type": "git",
    "url": "https://github.com/helsenorge/core-frontend-packages"
  },
  "main": "index.js",
  "publishConfig": {
    "directory": "lib",
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "node node_modules/@helsenorge/library-build/library-scripts.js copy --root=README.md",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "eslint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,scss,md,json}\"",
    "prettier:fix": "npm run prettier -- --write",
    "pretypecheck": "tsc -v",
    "typecheck": "tsc --noEmit"
  },
  "author": "Norsk helsenett SF",
  "maintainers": [
    "ROX"
  ],
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "peerDependencies": {
    "@helsenorge/designsystem-react": ">=12.0.0"
  },
  "devDependencies": {
    "@helsenorge/designsystem-react": "12.10.0",
    "@helsenorge/library-build": "^36.7.1",
    "@types/node": "^22.18.0"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "react-router": "^7.8.2"
  }
}
