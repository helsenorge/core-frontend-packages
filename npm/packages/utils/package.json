{
  "name": "@helsenorge/core-utils",
  "sideEffects": false,
  "version": "25.1.0",
  "description": "samling av verktÃ¸y for hele helsenorge",
  "main": "index.js",
  "publishConfig": {
    "directory": "lib"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc --p config/tsconfig.package.json",
    "postbuild": "node scripts/copy-files.cjs",
    "copy:libnpmjs": "node scripts/createNpmrc.cjs && copyfiles -u 1 \"./lib/**/*\" \"./lib_npm\" -E",
    "prepublishOnly": "npm run build",
    "pretest": "crlf --set=LF \"src/**/*.snap\"",
    "test": "node node_modules/@helsenorge/core-build/lib/helsenorge-scripts.js test",
    "test:update": "node node_modules/@helsenorge/core-build/lib/helsenorge-scripts.js test --u",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check src/**/*.{js,jsx,ts,tsx,css,scss,md,json}",
    "prettier:fix": "npm run prettier -- --write",
    "pretypecheck": "tsc -v",
    "typecheck": "tsc --noEmit",
    "test:watch": "node node_modules/@helsenorge/core-build/lib/helsenorge-scripts.js test --watch",
    "helsenorge-notify": "node node_modules/@helsenorge/core-build/lib/helsenorge-scripts.js notify"
  },
  "author": "Helsenorge",
  "maintainers": [
    "ROX"
  ],
  "license": "ISC",
  "type": "module",
  "peerDependencies": {
    "@helsenorge/designsystem-react": "^1.0.0"
  },
  "devDependencies": {
    "@helsenorge/core-build": "^25.0.0",
    "@helsenorge/designsystem-react": "1.0.1"
  },
  "dependencies": {
    "@types/classnames": "2.3.1",
    "classnames": "2.3.1",
    "moment": "^2.29.4",
    "tabbable": "5.2.1"
  },
  "browserslist": [
    "> 0.2% in NO",
    "not dead",
    "not op_mini all",
    "not ie > 0"
  ]
}
