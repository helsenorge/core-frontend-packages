{
  "name": "@helsenorge/core-utils",
  "sideEffects": false,
  "version": "23.5.0",
  "description": "samling av verktÃ¸y for hele helsenorge",
  "main": "index.js",
  "publishConfig": {
    "directory": "lib"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "node scripts/build.cjs",
    "postbuild": "rimraf lib/__devonly__",
    "prepublishOnly": "npm run build",
    "precommit-script": "pretty-quick --staged",
    "test": "node node_modules/@helsenorge/core-build/lib/helsenorge-scripts.js test",
    "test:update": "node node_modules/@helsenorge/core-build/lib/helsenorge-scripts.js test --u",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check src/**/*.{js,jsx,ts,tsx,css,scss,md,json}",
    "prettier:fix": "npm run prettier -- --write",
    "pretypecheck": "tsc -v",
    "typecheck": "tsc --noEmit",
    "test:watch": "node node_modules/@helsenorge/core-build/lib/helsenorge-scripts.js test --watch",
    "helsenorge-notify": "node node_modules/@helsenorge/core-build/lib/helsenorge-scripts.js notify"
  },
  "author": "Helsenorge",
  "maintainers": [
    "ROX"
  ],
  "license": "ISC",
  "type": "module",
  "peerDependencies": {
    "@helsenorge/designsystem-react": "^1.0.0-beta"
  },
  "devDependencies": {
    "@helsenorge/core-build": "^23.5.0",
    "@helsenorge/designsystem-react": "1.0.0-beta127"
  },
  "dependencies": {
    "moment": "^2.29.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "bash node_modules/@helsenorge/core-build/precommit.sh"
    }
  },
  "browserslist": [
    "> 0.2% in NO",
    "not dead",
    "not op_mini all",
    "not ie > 0"
  ]
