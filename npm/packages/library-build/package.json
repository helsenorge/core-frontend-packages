{
  "name": "@helsenorge/library-build",
  "version": "28.2.0",
  "publishConfig": {
    "directory": "lib",
    "access": "public"
  },
  "author": "Norsk helsenett SF",
  "maintainers": [
    "ROX"
  ],
  "description": "Bygg og test av komponentbibliotek",
  "license": "MIT",
  "repository": {
    "type": "tfs",
    "url": "https://dev.azure.com/nhnfelles/Helsenorge/_git/HN-Core-Frontend?path=%2Fnpm%2Fpublic%2Flibrary-build"
  },
  "bin": {
    "library-scripts": "./library-scripts.js"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "node lib/library-scripts.js copy --root=README.md --root=config/**/* --include=src/*.html",
    "watch": "tsc --watch",
    "eslint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "eslint:fix": "npm run lint -- --fix",
    "stylelint": "stylelint src/**/*.{css,scss}",
    "stylelint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check src/**/*.{js,jsx,ts,tsx,css,scss,md,json}",
    "prettier:fix": "npm run prettier -- --write",
    "pretypecheck": "tsc -v",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm-run-all --npm-path npm build prepublish:crlf",
    "prepublish:crlf": "crlf --set=LF lib/*"
  },
  "peerDependencies": {
    "core-js": ">=3.26.1"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.36"
  },
  "dependencies": {
    "@babel/core": "^7.21.8",
    "@helsenorge/eslint-config": "^28.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/copyfiles": "^2.4.1",
    "@types/enzyme": "^3.10.13",
    "@types/enzyme-to-json": "^1.5.4",
    "@types/react": "^17.0.59",
    "@types/react-dom": "^17.0.20",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "babel-jest": "^27.5.1",
    "babel-preset-react-app": "^10.0.1",
    "copyfiles": "2.4.1",
    "core-js": ">=3.30.2",
    "crlf": "1.1.1",
    "cross-spawn": "7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "esbuild": "^0.18.6",
    "esbuild-sass-plugin": "^2.10.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit-reporter": "^1.1.0",
    "jest-resolve": "^29.5.0",
    "postcss": "^8.4.23",
    "postcss-modules": "^6.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "^5.0.1",
    "tslib": "^2.5.2",
    "typescript": "~5.0.4"
  },
  "engineStrict": true,
  "engines": {
    "node": "^18.0.0",
    "npm": "^9.0.0"
  }
}
