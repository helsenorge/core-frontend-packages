{
  "name": "@helsenorge/library-build",
  "version": "30.0.0-beta.15",
  "publishConfig": {
    "directory": "lib",
    "access": "public"
  },
  "author": "Norsk helsenett SF",
  "maintainers": [
    "ROX"
  ],
  "description": "Bygg og test av komponentbibliotek",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/helsenorge/core-frontend-packages"
  },
  "bin": {
    "library-scripts": "./library-scripts.js"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "node lib/library-scripts.js copy --root=README.md --root=config/**/* --include=src/*.html",
    "watch": "tsc --watch",
    "eslint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier --check src/**/*.{js,jsx,ts,tsx,css,scss,md,json}",
    "prettier:fix": "npm run prettier -- --write",
    "pretypecheck": "tsc -v",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm-run-all --npm-path npm build prepublish:crlf",
    "prepublish:crlf": "crlf --set=LF lib/*"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.39"
  },
  "dependencies": {
    "@babel/core": "^7.23.7",
    "@helsenorge/eslint-config": "^30.0.0-beta.15",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.5.2",
    "@types/copyfiles": "^2.4.4",
    "@types/enzyme": "^3.10.18",
    "@types/enzyme-to-json": "^1.5.7",
    "@types/react": "^17.0.74",
    "@types/react-dom": "^17.0.25",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "babel-jest": "^29.7.0",
    "babel-preset-react-app": "^10.0.1",
    "copyfiles": "2.4.1",
    "crlf": "1.1.1",
    "cross-spawn": "7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "esbuild": "^0.19.11",
    "esbuild-sass-plugin": "^2.16.1",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit-reporter": "^1.1.0",
    "jest-resolve": "^29.7.0",
    "postcss": "^8.4.33",
    "postcss-modules": "^6.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "^5.0.5",
    "tslib": "^2.6.2",
    "typescript": "4.9.4"
  },
  "engineStrict": true,
  "engines": {
    "node": "^20.0.0",
    "npm": ">=10.0.0"
  }
}
