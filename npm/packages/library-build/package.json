{
  "name": "@helsenorge/library-build",
  "version": "35.0.0-beta.4",
  "publishConfig": {
    "directory": "lib",
    "access": "public"
  },
  "author": "Norsk helsenett SF",
  "maintainers": [
    "ROX"
  ],
  "description": "Bygg og test av komponentbibliotek",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/helsenorge/core-frontend-packages"
  },
  "bin": {
    "library-scripts": "./library-scripts.js"
  },
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "node lib/library-scripts.js copy --root=README.md --root=config/**/*",
    "watch": "tsc --watch",
    "eslint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,scss,md,json}\"",
    "prettier:fix": "npm run prettier -- --write",
    "pretypecheck": "tsc -v",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
    "@types/yargs": "^17.0.33",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "@helsenorge/eslint-config": "^35.0.0-beta.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/copyfiles": "^2.4.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitest/coverage-v8": "^3.0.8",
    "copyfiles": "^2.4.1",
    "cross-spawn": "^7.0.6",
    "esbuild": "~0.25.1",
    "jsdom": "^26.0.0",
    "rimraf": "^6.0.1",
    "sass": "^1.85.1",
    "sass-embedded": "^1.85.1",
    "typed-scss-modules": "^8.1.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8",
    "whatwg-fetch": "^3.6.20",
    "yargs": "^17.7.2"
  },
  "engineStrict": true,
  "engines": {
    "node": "^20.0.0",
    "npm": ">=10.0.0"
  }
}
