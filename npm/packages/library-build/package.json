{
  "name": "@helsenorge/library-build",
  "version": "33.0.0-beta.4",
  "publishConfig": {
    "directory": "lib",
    "access": "public"
  },
  "author": "Norsk helsenett SF",
  "maintainers": [
    "ROX"
  ],
  "description": "Bygg og test av komponentbibliotek",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/helsenorge/core-frontend-packages"
  },
  "bin": {
    "library-scripts": "./library-scripts.js"
  },
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "node lib/library-scripts.js copy --root=README.md --root=config/**/* --include=src/*.html",
    "watch": "tsc --watch",
    "eslint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier --check src/**/*.{js,jsx,ts,tsx,css,scss,md,json}",
    "prettier:fix": "npm run prettier -- --write",
    "pretypecheck": "tsc -v",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "@helsenorge/eslint-config": "^33.0.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/copyfiles": "^2.4.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitest/coverage-v8": "^2.1.3",
    "copyfiles": "^2.4.1",
    "cross-spawn": "^7.0.3",
    "esbuild": "^0.21.5",
    "esbuild-sass-plugin": "^3.3.1",
    "postcss": "^8.4.47",
    "postcss-modules": "^6.0.0",
    "rimraf": "^6.0.1",
    "typed-scss-modules": "^8.0.1",
    "typescript": "~5.5.4",
    "vitest": "^2.1.3",
    "whatwg-fetch": "^3.6.20"
  },
  "engineStrict": true,
  "engines": {
    "node": "^20.0.0",
    "npm": ">=10.0.0"
  }
}
