{
  "name": "@helsenorge/library-build",
  "version": "37.0.0",
  "publishConfig": {
    "directory": "lib",
    "access": "public"
  },
  "author": "Norsk helsenett SF",
  "maintainers": [
    "ROX"
  ],
  "description": "Bygg og test av komponentbibliotek",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/helsenorge/core-frontend-packages"
  },
  "bin": {
    "library-scripts": "./library-scripts.js"
  },
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "node lib/library-scripts.js copy --root=README.md --root=config/**/*",
    "watch": "tsc --watch",
    "eslint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,scss,md,json}\"",
    "prettier:fix": "npm run prettier -- --write",
    "pretypecheck": "tsc -v",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/yargs": "^17.0.35",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "dependencies": {
    "@helsenorge/eslint-config": "^37.0.0",
    "@helsenorge/typed-scss-modules": "^9.0.7",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/copyfiles": "^2.4.4",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "copyfiles": "^2.4.1",
    "esbuild": "~0.27.0",
    "jsdom": "^26.1.0",
    "node-fetch": "^3.3.2",
    "rimraf": "^6.1.2",
    "sass": "^1.94.2",
    "sass-embedded": "^1.93.3",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "yargs": "^18.0.0"
  },
  "engineStrict": true,
  "engines": {
    "node": "^22.0.0 || ^24.0.0",
    "npm": ">=10.0.0"
  }
}
